{
    "name": "open-junkfile",
    "displayName": "open Junkfile",
    "description": "",
    "version": "0.0.1",
    "publisher": "hidenba",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.openJunkfile"
    ],
    "main": "./out/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "OpenJunkfile configuration",
            "properties": {
                "junkfile.dir": {
                    "type": "string",
                    "default": "junk",
                    "description": "junkfile create directory"
                }
            }
        },
        "commands": [
            {
                "command": "extension.openJunkfile",
                "title": "Open Junkfile"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.3",
        "vscode": "^1.1.5",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
